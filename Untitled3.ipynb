    "import math\n",
    "#Metodo 3\n",
    "x = 4\n",
    "e2 = 0.0\n",
    "\n",
    "for i in range(10):\n",
    "    e2 += x**i/math.factorial(i)\n",
    "\n",
    "inver = 0\n",
    "inver = 1/e2\n",
    "\n",
    "print (\"Resulado de la serie: \", e2)\n",
    "print (\"Resultado de la serie inversa: \", inver)\n",
